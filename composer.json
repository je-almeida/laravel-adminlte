{
  "name": "je-almeida/laravel-adminlte",
  "description": "Easy AdminLTE integration with Laravel - By jeroennoten + updates",
  "keywords": ["laravel", "adminlte", "admin", "administrator"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jefferson F. de Almeida",
      "email": "jeff_almeida1992@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "JeAlmeida\\LaravelAdminLte\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/Console/CommandTestCase.php"
    ]
  },
  "extra": {
      "laravel": {
          "providers": [
              "JeAlmeida\\LaravelAdminLte\\AdminLteServiceProvider"
          ]
      }
  },
  "require": {
    "laravel/framework": ">=6.0",
    "php": ">=7.2.0",
    "almasaeed2010/adminlte": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "orchestra/testbench": ">=4.0"
  }
}
